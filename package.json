{
    "name": "codere-monorepo",
    "private": true,
    "type": "module",
    "version": "0.1.0",
    "workspaces": [
        "packages/*"
    ],
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "generate-impact": "npx code-impact build-graph --root",
        "analyze-impact": "npx code-impact impact --git-diff origin/main...HEAD --format mermaid --depth 4 > impact.mmd"
    }
}